  "private": true,
  "type": "module",
  "scripts": {
    "prepare": "cd .. && husky",
    "dev": "touch .env.development.local && react-router dev --port 3777 --host",
    "build": "react-router build",
    "dev:test": "VITE_TEST=true npm run dev",
    "build:test": "VITE_TEST=true npm run build",
    "start": "npx tsx test-server.ts",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "prettier": "prettier -u -c *",
    "prettier:fix": "prettier --write -u *",
    "test": "vitest",
    "typecheck": "react-router typegen && tsc",
    "test-playwright-docker": "./scripts/run-playwright-tests-in-docker.sh",
    "start-and-test-playwright-docker": "START_SERVER_AND_TEST_INSECURE=1 start-server-and-test start http://localhost:3777 'npm run test-playwright-docker -- $PLAYWRIGHT_ARGS'",
